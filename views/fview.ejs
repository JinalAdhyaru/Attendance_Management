<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <h1>Student Attendance</h1><br><br>
    <h2> <%= fview[0].cid %> <%= fview[0].cname %></h2>
    <table align="center">
        <tr>
            <th><b>student id</b></th>
            <th><b>student name</b></th>
            <th><b>date</b></th>
            <th><b>attendance</b></th>
        </tr>
        <% fview.forEach(function (user) { %>

        <tr>
            <td><input name="s_id" value="<%= user.sid %>" readonly></td>

            <td><input name="at_date" value="<%= user.at_date %>" readonly></td>

            <td><input name="attend" value="<%= user.attend %>" readonly></td>
        </tr>
        <% }) %>
    </table>
</body>
</html>